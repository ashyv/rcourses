<%-#<% if current_student %>

<h1> <%= @student.name %>'s dashboard ! </h1>
<!-- <h2> Your cal id is: <%= @student.cal_id %> </h2> -->

<%-#<% end %>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  	<h3> <%= @student.name %>'s courses </h3>
    <% @student_courses.each do |scourse| %>
    	<% if scourse.student_id == @student.id %> 
      <tr>
        <td><%= Course.find(scourse.course_id).name %></td>
        <td><%= link_to 'Show', course_path(scourse.course_id) %></td>
      </tr>
      <% end %>
    <% end %>
  </tbody>
</table>
<br>
