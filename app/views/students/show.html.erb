<%-#<% if current_student %>

<h1> <%= @student.name %>'s dashboard ! </h1>
<!-- <h2> Your cal id is: <%= @student.cal_id %> </h2> -->

<%-#<% end %>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  	<h3> <%= @student.name %>'s courses </h3>
    <% @student.courses.each do |course| %>
      <tr>
        <td><%= course.name %></td>
        <td><%= link_to 'Show', course_path(course) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
